{
    "name": "waytohealth/oauth2-omron",
    "description": "Omron OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Way To Health",
            "email": "dev@waytohealth.org",
            "homepage": "https://www.waytohealth.org"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "omron"
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "2.4.*",
        "firebase/php-jwt": "5.2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.0",
        "friendsofphp/php-cs-fixer": "~1.0",
        "jakub-onderka/php-parallel-lint": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "waytohealth\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "waytohealth\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
